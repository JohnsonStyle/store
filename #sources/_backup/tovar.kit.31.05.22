    <style>
        .sp-img_area {
            -webkit-box-orient: unset;
            -webkit-box-direction: unset;
            -webkit-flex-direction: unset;
            -ms-flex-direction: unset;
            flex-direction: unset;
            display: -webkit-box;
            display: -webkit-flex;
            display: -ms-flexbox;
            display: flex;


            -webkit-box-align: baseline;
            -webkit-align-items: baseline;
            -ms-flex-align: baseline;
            align-items: baseline;
        }

        .sp-img_area .slick-vertical {
            -webkit-flex-basis: 20%;
            -ms-flex-preferred-size: 20%;
            flex-basis: 29%;
            max-width: 20%;
            padding: 0;
            margin-top: 0;
            margin-right: 10px;
        }

        .sp-img_area .slider-for {

            -webkit-flex-basis: 90%;
            -ms-flex-preferred-size: 90%;
            flex-basis: 90%;
            max-width: 90%;
            padding-left: 20px;
        }


        .sp-img_area .slick-vertical img {
            border: 1px solid #e5e5e5;
            padding: 10px 0;
            opacity: 0.8;
            filter: grayscale(0.5);
            cursor: pointer;
        }

        .sp-img_area .slick-vertical .slick-current img {
            border-color: #c1a69a;
            opacity: 1;
            filter: grayscale(0);
            cursor: pointer;

        }


        .slick-prev {}

        .slick-arrow {
            background: #c1a69a;
            border: 0px;
            color: #ffffff;
            display: -webkit-box;
            display: -webkit-flex;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-pack: center;
            -webkit-justify-content: center;
            -ms-flex-pack: center;
            justify-content: center;
            width: 100%;
            font-size: 20px;
        }




        .zoom img {
            display: block;
            cursor: crosshair;
        }

        .zoom img::selection {
            background-color: transparent;
        }


        .slider-nav .icons {
            position: absolute;
            z-index: 999;
            font-size: 47px;
            margin: 25px 10px;
            color: #4e4039;
            opacity: 0.5;
            cursor: pointer;

        }

        h1.title {
            font-size: 1.6em;
        }

        .product-description input[type="radio"]+label {
            margin-left: 0px;
            cursor: pointer;
            float: left;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .product-description input[type="radio"] {
            left: -9999px;
            position: absolute;
        }

        .product-description label {
            background-position: center;
            background-repeat: no-repeat;
            text-align: center;
            /*                        line-height: 60px;*/
            font-size: 10px;
            padding: 1px 1px 1px 1px;
            border: 0px solid #fff;
            width: 65px;
            height: 65px;
            color: #fff;
        }

        .product-description label span {
            line-height: 1em;
            padding-top: 23px;
        }

        .product-description label {
            background-color: #fff;
            color: #191A19 !important;
            border: 2px solid #fff;
            border-radius: 5px;
        }

        .product-description label:hover,
        .product-description input[type="radio"]:checked+label {
            font-weight: normal;
            border: 2px solid #292323;
            border-radius: 5px;
            outline: none !important;
            transition: all 0.5s;
        }

        .product-description label span {
            width: 58px;
            height: 58px;
            opacity: 1;
            transition: all 0.5s;
            display: block;
            border-radius: 3px;
        }

        .product-description label:hover span {
            opacity: 1.0;
            transition: all 0.5s;
        }

        .product-description label:hover span .prod-options-descr {
            display: block !important
        }

        .prod-options-descr {
            display: none;
            color: #333;
            padding: 8px 22px;
            background-color: #fff;
            border: 1px solid #d2d2d2;
            box-shadow: 0 2px 2px rgba(0, 0, 0, .15);
            border-radius: 7px;
            position: relative;
            top: 19px;
            left: 0;
            width: 225px;
            z-index: 2;
            margin-left: -75px;
        }

        .prod-options-descr .image {
            margin-bottom: 22px;
            position: relative;
        }

        .prod-options-descr img {
            width: 191px;
            height: 191px;
        }

        .prod-options-descr:before {
            top: -14px;
            border-bottom: 13px solid #d2d2d2;
        }



        .prod-options-descr .image:before {
            display: block;
            content: '';
            width: 219px;
            height: 221px;
            position: absolute;
            top: 1;
            left: -17px;
