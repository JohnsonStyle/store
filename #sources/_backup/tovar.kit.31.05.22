                    <script type="text/javascript">
                        $(document).ready(function() {
                            $('.slider-for').slick({
                                slidesToShow: 1,
                                slidesToScroll: 1,
                                arrows: false,
                                fade: true,
                                asNavFor: '.slider-nav'
                            });
                            $('.slider-nav').slick({
                                prevArrow: '<button type="button" class="slick-prev"><i class="bx bx-chevron-up"></i></button>',
                                nextArrow: '<button type="button" class="slick-prev"><i class="bx bx-chevron-down"></i></button>',
                                slidesToShow: 10,
                                slidesToScroll: 1,
                                asNavFor: '.slider-for',
                                dots: false,
                                infinite: false,
                                focusOnSelect: true,
                                arrows: true,
                                adaptiveHeight: true,
                                vertical: true,
                                verticalSwiping: true,
                                responsive: [{
                                    breakpoint: 992,
                                    settings: {
                                        slidesToShow: 3,
                                        vertical: true,
                                        verticalSwiping: true,
                                    }
                                }, ],
                            });

 //default
                            let tkaninaCode = $("#colors .option input ").first().val().replace(/\d+/g, ''),
                                colorCode = $("#colors .option input ").first().val().replace(/[^0-9]/gi, ''),
                                numImg = '1',
                                nozki = $("#nozki .option input ").first().val(),
                                pin = $("#pin .option input ").first().val(),
                                urlPin ='/img/options/pin/' +  pin + '.png',
                                urlNoz ='/img/options/nozki/1+' +  nozki + '.png',
                                url = ('/img/product2/' + tkaninaCode + '/' + colorCode + '/1+baza.jpg').replace(/\s+/g, '');
                                //numImg взяти з активної .slick-track   яка активна slick-current
                                console.log(tkaninaCode, colorCode, numImg, nozki)
//багнутий скрипт ЗУМ
                            
                                
                            
                           


                            $("#nozki .option input ").click(function() {
                                nozki = $(this).val(); //берем кодножки (1:1 назва файла і код)кольору 

                                console.log(nozki);

                               imgUrlNoz(); //ф-ція прогрузки url

                                //загрузка по кліку zoom reset
                               render();
                                setTimeout(loadZoom, 10);
                            })
                            
                            
                            $("#pin .option input ").click(function() {
                                pin = $(this).val(); //берем код pineska (1:1 назва файла і код)  
                                imgUrlPin(pin);
                               render();
                                setTimeout(loadZoom, 10);
                            })



                            $("#colors .option input ").click(function() {
                                colorCode = $(this).val().replace(/[^0-9]/gi, ''); //берем код кольору 
                                tkaninaCode = $(this).val().replace(/\d+/g, ''); //берем текст колекція тканини 
                                //                                    console.log($('single-color').index( this ));
                                imgUrl(tkaninaCode, colorCode); //ф-ція прогрузки url
                                //загрузка по кліку zoom reset
                                 render();
                                setTimeout(loadZoom, 10);

                               
                            })


                            function loadZoom() {
                                    $('.zoom').trigger('zoom.destroy');
                                    $('.zoom').zoom();
                                   
                                }



                            function imgUrl(t, c, n) {
                                url = ('/img/product2/' + tkaninaCode + '/' + colorCode + '/1+baza.jpg').replace(/\s+/g, '');
                                //$('img.tovar').attr('src', url);
                               // console.log(url)
                            }
                            
                             function imgUrlPin() {
                                 urlPin = ('/img/options/pin/' + pin + '.png').replace(/\s+/g, '');
                                  //    console.log(urlPin)
                            }
                             function imgUrlNoz() {
                                 urlNoz = ('/img/options/nozki/1+' + nozki + '.png').replace(/\s+/g, '');
                                   //   console.log(urlNoz)
                            }   

                       
                         
                       
                        
                        // just the assets loader
                        //(async () => {
                        //    [  '/img/options/pin/pin_g.png',urlPin
                        //       '/img/options/pin/pin_g.png',
                        //       '/img/options/pin/pin_g.png'
                        //    ]
                          async function render(){  
                              const [nozka, face, border, back] = await Promise.all(
                                [
                                    urlNoz,
                                    urlPin, 
                                    url,
                                    url
                                ].map(url => {
                                    const img = new Image();
                                    img.src = url;
                                    return new Promise(res => {
                                        img.onload = e => res(img);
                                    });
                                })
                            );


                            // ctx = document.getElementById('canvas').getContext('2d');
                            const canvas = document.getElementById("canvas")
                            const ctx = canvas.getContext("2d");

                            ctx.drawImage(border, 0, 0);
                            //  await wait(1100); // draw the face only where we already have the border drawn
                            ctx.globalCompositeOperation = 'source-in'; //source-in

                                                  
                             ctx.drawImage(nozka, 0, 0);
                            //  await wait(1100); // draw the background behind
                            ctx.globalCompositeOperation = 'destination-over'; 
                                                  
                                                  
                            ctx.drawImage(face, 0, 0);
                            // await wait(1100); // draw the background behind
                            ctx.globalCompositeOperation = 'destination-over';
                            
                            ctx.drawImage(back, 0, 0, );

                            // reset
                            ctx.globalCompositeOperation = 'source-over';

                            var renderImg = new Image();
                           renderImg.src = canvas.toDataURL('image/jpeg', 0.99); //type + encode quality 
                            document.getElementById('bImg').innerHTML = '';
                            document.getElementById('bImg').appendChild(renderImg).classList.add("img-fluid");
                            
                                                  
                                  
                           $('.zoom').zoom(); //загрузка мадуля при старті сторінки
                                                  }; 
                            render(); // })();


                         function wait(ms) { return new Promise(res => setTimeout(res, ms)) }
                            
                            
             });
                    </script>



